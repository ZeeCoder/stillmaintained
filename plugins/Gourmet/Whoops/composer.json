{
	"name": "gourmet/whoops"
	, "description": "Whoops for CakePHP 3"
	, "version": "0.1.1"
	, "type": "cakephp-plugin"
	, "keywords": [
		"cakephp"
		, "whoops"
	]
	, "homepage": "http://github.com/gourmet/whoops"
	, "license": "MIT"
	, "authors": [
		{
			"name": "Jad Bitar"
			, "homepage": "http://jadb.io"
			, "role": "Author"
		}
	]
	, "support": {
		"issues": "http://github.com/gourmet/whoops/issues"
		, "source": "http://github.com/gourmet/whoops"
	}
	, "require": {
		"composer/installers": "*"
		, "filp/whoops": "~1.0"
	}
	, "suggest": {
		"phpunit/phpunit": "Allows automated tests to be run without system-wide install."
	}
	, "autoload": {
		"psr-4": {
			"Gourmet\\Whoops\\": "."
		}
	}
	, "extra": {
		"installer-name": "Gourmet/Whoops"
	}
}
